{
    "name": "prasso/messaging",
    "description": "messaging functionality for Laravel projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Prasso\\Messaging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Prasso\\Messaging\\MessagingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
